{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true,
    "editor.insertSpaces": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": [
      "document"
    ]
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "chat.agent.maxRequests": 100,
  "chat.tools.autoApprove": true,
  "coderabbit.autoReviewMode": "disabled",
  "cSpell.enabledNotifications": {
    "Average Word Length too Long": false,
    "Lines too Long": false,
    "Maximum Word Length Exceeded": false
  },
  "cSpell.userWords": [
    "Barbhuiya",
    "Hussain",
    "Imran",
    "imranbarbhuiya",
    "keyspace",
    "lngs",
    "mehrin",
    "Middlewares",
    "nextjs",
    "parbez",
    "paren",
    "parens",
    "preconnect"
  ],
  "diffEditor.diffAlgorithm": "advanced",
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultColorDecorators": "auto",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Monaspace Neon', 'JetBrains Mono', 'FiraCode Nerd Font'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.linkedEditing": true,
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.suggestSelection": "first",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html",
    "vue-html": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".env.local": ".env.*",
    ".eslintrc.json": ".eslint*, .prettier*",
    ".gitignore": ".gitattributes, .gitmodules",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.mjs, ${capture}.global.js, ${capture}.test.js",
    "*.jsx": "${capture}.js",
    "*.mjs": "${capture}.mjs.map, ${capture}.min.mjs, ${capture}.d.mts",
    "*.schema.ts": "${capture}.model.ts, ${capture}.dto.ts",
    "*.ts": "${capture}.test.ts, ${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.test.tsx, ${capture}.test.ts, ${capture}.module.css, ${capture}.css",
    "eslint.config.js": ".eslint*, .prettier*, biome.json",
    "eslint.config.mjs": ".eslint*, .prettier*, biome.json",
    "jest.config.cjs": "jest-*.cjs, jest.*.cjs",
    "jest.config.js": "jest-*.js, jest.*.js",
    "jest.config.mjs": "jest-*.mjs, jest.*.mjs",
    "package.json": "package-lock.json, yarn.lock, .yarnrc.yml, turbo.json",
    "tsconfig.base.json": "tsconfig.*.json",
    "tsconfig.json": "tsconfig.*.json",
    "vitest.config.ts": "vitest-*.ts, vitest.*.ts"
  },
  "files.associations": {
    "*.html": "html"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*/**": true,
    "**/target/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.advanced": {
    "slashCommands": {
      "createNotebook": true,
      "createWorkspace": true,
      "search": true
    }
  },
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "Use conventional commit message format."
    }
  ],
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true,
    "scminput": true
  },
  "github.copilot.nextEditSuggestions.enabled": false,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "githubPullRequests.pullBranch": "never",
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4o",
  "gitlens.defaultDateFormat": null,
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.views.branches.branches.layout": "list",
  "go.toolsManagement.autoUpdate": true,
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.suggest.autoImports": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "mdx.validate.validateFileLinks": "ignore",
  "mdx.validate.validateFragmentLinks": "ignore",
  "mdx.validate.validateReferences": "ignore",
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.defaultExtensionsIfInstalledLocally": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode"
  ],
  "remote.SSH.remotePlatform": {},
  "remoteHub.commitDirectlyWarning": "off",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": [
    "--all",
    "--",
    "-D",
    "warnings"
  ],
  "search.exclude": {
    "**/.next": true,
    "**/.yarn": true,
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.autoReplies": {
    "Terminate batch job (Y/N)": "Y\r"
  },
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "'JetBrainsMono Nerd Font', 'FiraCode Nerd Font'",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.suggest.cdPath": "relative",
  "terminal.integrated.suggest.enabled": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "SECURITY",
    "PRODUCTION"
  ],
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#d800f0",
      "foreground": "#000000"
    },
    "PRODUCTION": {
      "background": "#00ff00",
      "foreground": "#000000",
      "icon": "check",
      "iconColor": "green"
    },
    "SECURITY": {
      "background": "#ff0000",
      "foreground": "#000000",
      "icon": "alert",
      "iconColor": "red"
    },
    "TODO": {
      "background": "#ffd700",
      "foreground": "#000000"
    }
  },
  "typescript.autoClosingTags": true,
  "typescript.experimental.expandableHover": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.suggest.autoImports": true,
  "typescript.tsserver.watchOptions": "vscode",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnPaste.enabled": false,
  "window.newWindowDimensions": "maximized",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "bottom",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.editor.customLabels.patterns": {
    "**/app/**/_layout.tsx": "${dirname} - Layout",
    "**/app/**/index.tsx": "${dirname} - Page",
    "**/app/**/layout.tsx": "${dirname} - Layout",
    "**/app/**/page.tsx": "${dirname} - Page",
    "**/app/**/route.tsx": "${dirname} - Route",
    "**/components/**/index.tsx": "${dirname} - Component"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.secondarySideBar.showLabels": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zig.zls.enabled": "on"
}
