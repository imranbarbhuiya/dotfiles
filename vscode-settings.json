{
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "always"
    },
    "editor.defaultFormatter": "golang.go",
    "editor.formatOnSave": true,
    "editor.insertSpaces": false
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features",
    "editor.suggest.insertMode": "replace",
    "gitlens.codeLens.scopes": ["document"]
  },
  "[python]": {
    "editor.formatOnType": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "chat.agent.maxRequests": 100,
  "chat.editor.wordWrap": "on",
  "chat.todoListTool.enabled": true,
  "chat.tools.autoApprove": true,
  "chat.tools.terminal.autoApprove": {
    "/Compare-[a-z0-9]/i": true,
    "/find\\b.*-exec(dir)?\\b/": false,
    "/Format-[a-z0-9]/i": true,
    "/Get-[a-z0-9]/i": true,
    "/Measure-[a-z0-9]/i": true,
    "/Select-[a-z0-9]/i": true,
    "/Sort-[a-z0-9]/i": true,
    "/top\\b.*-(k|r)\\b/": false,
    "basename": true,
    "bun add": true,
    "bun build": false,
    "bun dev": false,
    "bun i(nstall)?": true,
    "bun remove": true,
    "bun run": false,
    "bun start": false,
    "cat": true,
    "chmod": false,
    "chown": false,
    "cmp": true,
    "column": true,
    "curl": false,
    "cut": true,
    "date": true,
    "del": false,
    "df": true,
    "dirname": true,
    "du": true,
    "echo": true,
    "env": true,
    "erase": false,
    "eval": false,
    "file": true,
    "find": true,
    "findstr": true,
    "grep": true,
    "head": true,
    "hostname": true,
    "iex": false,
    "Invoke-Expression": false,
    "Invoke-RestMethod": false,
    "Invoke-WebRequest": false,
    "irm": false,
    "iwr": false,
    "Join-Path": true,
    "jq": true,
    "kill": false,
    "ls": true,
    "npm": false,
    "npx": true,
    "pnpm add": true,
    "pnpm build": false,
    "pnpm dev": false,
    "pnpm i(nstall)?": true,
    "pnpm remove": true,
    "pnpm run": false,
    "pnpm start": false,
    "printenv": true,
    "ps": true,
    "pwd": true,
    "rd": false,
    "readlink": true,
    "realpath": true,
    "Remove-Item": false,
    "ri": false,
    "rm": null,
    "rmdir": null,
    "Set-Acl": false,
    "Set-ItemProperty": false,
    "sleep": true,
    "sort": true,
    "sp": false,
    "Split-Path": true,
    "spps": false,
    "Start-Sleep": true,
    "stat": true,
    "Stop-Process": false,
    "tail": true,
    "taskkill": false,
    "taskkill.exe": false,
    "time": true,
    "top": true,
    "tr": true,
    "tree": true,
    "uname": true,
    "uniq": true,
    "wc": true,
    "wget": false,
    "which": true,
    "whoami": true,
    "Write-Host": true,
    "Write-Output": true,
    "yarn add": true,
    "yarn build": false,
    "yarn dev": false,
    "yarn i(nstall)?": true,
    "yarn remove": true,
    "yarn run": false,
    "yarn start": false
  },
  "coderabbit.autoReviewMode": "disabled",
  "cSpell.enabledNotifications": {
    "Average Word Length too Long": false,
    "Lines too Long": false,
    "Maximum Word Length Exceeded": false
  },
  "cSpell.userWords": [
    "Barbhuiya",
    "Hussain",
    "Imran",
    "imranbarbhuiya",
    "keyspace",
    "lngs",
    "nextjs",
    "preconnect"
  ],
  "diffEditor.experimental.showMoves": true,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "Lilex, 'JetBrains Mono'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlayHints.enabled": "on",
  "editor.inlineSuggest.enabled": true,
  "editor.inlineSuggest.suppressSuggestions": true,
  "editor.lineNumbers": "relative",
  "editor.linkedEditing": true,
  "editor.minimap.autohide": "mouseover",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 1,
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "markdown": "html",
    "vue-html": "html"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    ".env.local": ".env.*",
    ".eslintrc.json": ".eslint*, .prettier*",
    ".gitignore": ".gitattributes, .gitmodules",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts, ${capture}.mjs, ${capture}.global.js, ${capture}.test.js",
    "*.jsx": "${capture}.js",
    "*.mjs": "${capture}.mjs.map, ${capture}.min.mjs, ${capture}.d.mts",
    "*.schema.ts": "${capture}.model.ts, ${capture}.dto.ts",
    "*.ts": "${capture}.test.ts, ${capture}.js",
    "*.tsx": "${capture}.ts, ${capture}.test.tsx, ${capture}.test.ts, ${capture}.module.css, ${capture}.css",
    "eslint.config.js": ".eslint*, .prettier*, biome.json",
    "eslint.config.mjs": ".eslint*, .prettier*, biome.json",
    "jest.config.cjs": "jest-*.cjs, jest.*.cjs",
    "jest.config.js": "jest-*.js, jest.*.js",
    "jest.config.mjs": "jest-*.mjs, jest.*.mjs",
    "package.json": "package-lock.json, yarn.lock, .yarnrc.yml, turbo.json",
    "tsconfig.base.json": "tsconfig.*.json",
    "tsconfig.json": "tsconfig.*.json",
    "vitest.config.ts": "vitest-*.ts, vitest.*.ts"
  },
  "files.associations": {
    "*.html": "html"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*/**": true,
    "**/target/**": true
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableCommitSigning": true,
  "git.ignoreRebaseWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "github.copilot.chat.alternateGptPrompt.enabled": true,
  "github.copilot.chat.commitMessageGeneration.instructions": [
    {
      "text": "Use conventional commit message format."
    }
  ],
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": true,
    "scminput": true
  },
  "github.copilot.nextEditSuggestions.enabled": false,
  "github.copilot.selectedCompletionModel": "gpt-4o-copilot",
  "githubPullRequests.pullBranch": "never",
  "githubPullRequests.terminalLinksHandler": "github",
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4o",
  "gitlens.defaultDateFormat": null,
  "gitlens.launchpad.indicator.enabled": false,
  "gitlens.views.branches.branches.layout": "list",
  "go.toolsManagement.autoUpdate": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.singleQuote": true,
  "redhat.telemetry.enabled": false,
  "remote.defaultExtensionsIfInstalledLocally": [
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode"
  ],
  "remote.SSH.remotePlatform": {},
  "remoteHub.commitDirectlyWarning": "off",
  "rust-analyzer.check.command": "clippy",
  "rust-analyzer.check.extraArgs": ["--all", "--", "-D", "warnings"],
  "search.exclude": {
    "**/.next": true,
    "**/.yarn": true,
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/yarn.lock": true
  },
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.autoReplies": {
    "Terminate batch job (Y/N)": "Y\r"
  },
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "Lilex, 'JetBrainsMono Nerd Font'",
  "terminal.integrated.fontLigatures.enabled": true,
  "terminal.integrated.shellIntegration.enabled": true,
  "terminal.integrated.suggest.cdPath": "relative",
  "terminal.integrated.suggest.enabled": true,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "SECURITY",
    "PRODUCTION"
  ],
  "todo-tree.highlights.customHighlight": {
    "FIXME": {
      "background": "#d800f0",
      "foreground": "#000000"
    },
    "PRODUCTION": {
      "background": "#00ff00",
      "foreground": "#000000",
      "icon": "check",
      "iconColor": "green"
    },
    "SECURITY": {
      "background": "#ff0000",
      "foreground": "#000000",
      "icon": "alert",
      "iconColor": "red"
    },
    "TODO": {
      "background": "#ffd700",
      "foreground": "#000000"
    }
  },
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.preferences.preferTypeOnlyAutoImports": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnPaste.enabled": false,
  "vim.overrideCopy": false,
  "window.newWindowDimensions": "maximized",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.location": "bottom",
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.editor.customLabels.patterns": {
    "**/app/**/_layout.tsx": "${dirname} - Layout",
    "**/app/**/index.tsx": "${dirname} - Page",
    "**/app/**/layout.tsx": "${dirname} - Layout",
    "**/app/**/page.tsx": "${dirname} - Page",
    "**/app/**/route.tsx": "${dirname} - Route",
    "**/components/**/index.tsx": "${dirname} - Component"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.secondarySideBar.showLabels": false,
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "zig.zls.enabled": "on"
}
